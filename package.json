{
	"name": "storybook-addon-grid",
	"version": "0.0.14",
	"description": "Column guides for your stories",
	"keywords": [
		"storybook",
		"storybook-addon",
		"columns",
		"grids",
		"guides",
		"design"
	],
	"repository": "maraisr/storybook-addon-grid",
	"license": "MIT",
	"author": {
		"name": "Marais Rossouw",
		"email": "me@marais.dev",
		"url": "https://marais.io"
	},
	"sideEffects": false,
	"exports": {
		"./preset": {
			"import": "./dist/preset.mjs",
			"require": "./dist/preset.js"
		},
		"./chromatic": {
			"import": "./dist/chromatic.mjs",
			"require": "./dist/chromatic.js"
		},
		"./package.json": "./package.json"
	},
	"main": "dist/preset.js",
	"module": "dist/preset.mjs",
	"types": "index.d.ts",
	"files": [
		"dist",
		"preset.js",
		"preset.mjs",
		"chromatic.js",
		"chromatic.mjs",
		"index.d.ts"
	],
	"scripts": {
		"build": "node bin",
		"format": "prettier  --write --list-different \"{*,{src,example,bin,.github}/**/*}.+(ts||tsx|js|json|yml|md)\"",
		"example": "concurrently -kr \"pnpm start-storybook --config-dir example/.storybook --port 8080  --no-manager-cache\" \"nodemon --ext tsx,ts --ignore dist/ --watch src bin\"",
		"prepublishOnly": "pnpm run build",
		"typecheck": "tsc --noEmit"
	},
	"prettier": {
		"bracketSpacing": true,
		"printWidth": 80,
		"proseWrap": "always",
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true,
		"overrides": [
			{
				"files": [
					"*.md",
					"*.yml"
				],
				"options": {
					"tabWidth": 2,
					"printWidth": 120,
					"useTabs": false
				}
			}
		]
	},
	"dependencies": {
		"@emotion/cache": "^10",
		"@emotion/core": "^10",
		"@storybook/addons": "^6.4.21",
		"@storybook/api": "^6.4.21",
		"@storybook/components": "^6.4.21",
		"@storybook/core-events": "^6.4.21",
		"@storybook/theming": "^6.4.21",
		"@theuiteam/continuous-container": "^1.0.0"
	},
	"devDependencies": {
		"@marais/tsconfig": "0.0.2",
		"@storybook/builder-webpack5": "6.4.21",
		"@storybook/cli": "6.4.21",
		"@storybook/manager-webpack5": "6.4.21",
		"@storybook/react": "6.4.21",
		"@types/node": "17.0.23",
		"@types/react": "17.0.39",
		"@types/react-dom": "17.0.11",
		"chromatic": "6.5.4",
		"concurrently": "7.1.0",
		"cross-env": "7.0.3",
		"esbuild": "0.14.34",
		"nodemon": "2.0.15",
		"prettier": "2.6.2",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"typescript": "4.6.3",
		"webpack": "5.72.0"
	},
	"peerDependencies": {
		"@types/react": ">=16.9.0",
		"@types/react-dom": ">=16.9.0",
		"chromatic": "*",
		"react": ">=16.9.0",
		"react-dom": ">=16.9.0"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/react-dom": {
			"optional": true
		},
		"chromatic": {
			"optional": true
		}
	},
	"storybook": {
		"displayName": "Column Guides",
		"icon": "https://raw.githubusercontent.com/maraisr/storybook-addon-grid/main/shots/square_logo.png",
		"supportedFrameworks": [
			"React"
		]
	},
	"volta": {
		"node": "16.2.0"
	}
}
